{
  "name": "eo-logger",
  "version": "1.1.1",
  "main": "dist/index.js",
  "repository": "https://github.com/ibitcy/eo-logger.git",
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "engines": {
    "node": ">= 10"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "postbuild": "size-limit",
    "preversion": "npm run build"
  },
